{
  "expo": {
    "name": "BCIS",
    "description": "The application for BCIS school, developed by Parin from LINK Student Council.",
    "slug": "bcis-sc-app",
    "owner": "parinzee",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "bcis",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f9ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parinzee.bcis-sc-app",
      "buildNumber": "1.0.2"
    },
    "android": {
      "package": "com.parinzee.bcis_sc_app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
